{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node --max-old-space-size=4096 dist/api/src/main --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint src/",
    "typecheck": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration": "pnpm mikro-orm migration:create",
    "up": "pnpm mikro-orm migration:up"
  },
  "dependencies": {
    "@mikro-orm/core": "6.5.9",
    "@mikro-orm/migrations": "6.5.9",
    "@mikro-orm/nestjs": "6.1.1",
    "@mikro-orm/postgresql": "6.5.9",
    "@mikro-orm/reflection": "^6.5.9",
    "@nestjs/common": "11.0.0",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.0.0",
    "@nestjs/jwt": "11.0.1",
    "@nestjs/platform-express": "11.0.0",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "11.2.1",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/nest": "3.52.1",
    "axios": "^1.13.2",
    "bcrypt": "6.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cloudinary": "^2.8.0",
    "crypto": "1.0.1",
    "dotenv": "^17.2.3",
    "expo-server-sdk": "^4.0.0",
    "express": "5.1.0",
    "jsonwebtoken": "9.0.2",
    "mikro-orm": "6.5.8",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@mikro-orm/cli": "6.5.9",
    "@nestjs/cli": "11.0.10",
    "@nestjs/testing": "11.0.0",
    "@types/bcrypt": "6",
    "@types/express": "5",
    "ts-node": "10.9.2",
    "tsc-esm-fix": "3.1.2",
    "tsconfig-paths": "4.2.0"
  }
}
